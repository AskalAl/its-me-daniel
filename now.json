{
  "name": "its-me-daniel",
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404"
    }
  ]
}