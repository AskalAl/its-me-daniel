{
  "name": "its-me-daniel",
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_API_KEY": "@REACT_APP_API_KEY",
      "REACT_APP_API_URL": "@REACT_APP_API_URL"
    }
  }
}